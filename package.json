{
  "name": "proxy",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "16.17.0"
  },
  "scripts": {
    "dev": "nodemon ./index.js",
    "prod": "node ./index.js",
    "lint": "eslint *.js lib/*.js --fix",
    "start": "NODE_ENV=development nf start --procfile ./Procfile_dev --port 3333",
    "get-block-id": "NODE_ENV=development node ./lib/notion/get-block-id.js",
    "add-year": "NODE_ENV=development node ./lib/notion/add-year.js",
    "year-in-review": "NODE_ENV=development node ./lib/notion/year-in-review.js",
    "dev-script": "NODE_ENV=development node ./scripts/run-script",
    "daytime-hourly": "node ./scripts/run-daytime-hourly",
    "daytime-quarter-hourly": "node ./scripts/run-daytime-quarter-hourly",
    "nightly": "node ./scripts/run-nightly"
  },
  "nodemonConfig": {
    "ignore": [
      "*.json"
    ]
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dayjs": "^1.10.6",
    "debug": "^4.3.1",
    "express": "^4.16.2",
    "express-handlebars": "^5.3.0",
    "fs-extra": "^10.0.0",
    "minimist": "^1.2.5",
    "morgan": "^1.10.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "socket.io": "^3.1.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-crb": "^2.0.3",
    "foreman": "^2.0.0",
    "nodemon": "^1.14.11"
  }
}
