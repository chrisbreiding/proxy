{
  "name": "proxy",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "16.18.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "watch": "tsc --watch",
    "deploy": "git push dokku master",
    "ts-check": "tsc --noEmit",
    "lint": "eslint *.ts lib/*.ts lib/**/*.ts lib/**/*.js",
    "lint-fix": "npm run lint -- --fix",
    "lint-staged": "lint-staged",
    "postinstall": "patch-package",
    "prepare": "husky install",
    "prod": "TZ='America/New_York' node ./dist/index.js",
    "start": "TZ='America/New_York' NODE_ENV=development DEBUG=proxy nf start --procfile ./Procfile_dev --port 3333",
    "dev": "TZ='America/New_York' DEBUG=proxy,proxy:verbose nodemon ./index.ts",
    "test": "npm run ts-check && TZ='America/New_York' vitest run --coverage",
    "test-single": "NODE_ENV=development TZ='America/New_York' vitest run",
    "test-watch": "TZ='America/New_York' vitest",
    "dev-script": "NODE_ENV=development DEBUG=proxy,proxy:verbose node ./scripts/run-script",
    "daytime-hourly": "DEBUG=proxy node ./scripts/run-daytime-hourly",
    "daytime-quarter-hourly": "DEBUG=proxy node ./scripts/run-daytime-quarter-hourly",
    "nightly": "DEBUG=proxy node ./scripts/run-nightly"
  },
  "lint-staged": {
    "*.{js,ts}": "npm run lint"
  },
  "nodemonConfig": {
    "ignore": [
      "*.json"
    ]
  },
  "dependencies": {
    "axios": "^1.1.3",
    "body-parser": "^1.20.1",
    "bree": "^9.1.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.6",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "express-handlebars": "^6.0.6",
    "firebase-admin": "^11.2.0",
    "fs-extra": "^10.1.0",
    "minimist": "^1.2.7",
    "mixpanel": "^0.17.0",
    "morgan": "^1.10.0",
    "socket.io": "^4.5.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@notionhq/client": "^2.2.1",
    "@types/body-parser": "^1.19.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.14",
    "@types/fs-extra": "^9.0.13",
    "@types/minimist": "^1.2.2",
    "@types/mock-fs": "^4.13.1",
    "@types/morgan": "^1.9.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitest/coverage-c8": "^0.25.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-crb": "^2.0.3",
    "foreman": "^3.0.1",
    "get-port": "^6.1.2",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "mock-fs": "^5.2.0",
    "nock": "^13.2.9",
    "nodemon": "^2.0.20",
    "patch-package": "^6.4.7",
    "supertest": "^6.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vitest": "^0.24.3"
  }
}
