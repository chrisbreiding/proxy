{
  "name": "proxy",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "24.x"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build-factor-bookmarklet": "NODE_ENV=development ts-node ./scripts/bundle-factor-bookmarklet.ts",
    "watch-factor-bookmarklet": "NODE_ENV=development ts-node ./scripts/bundle-factor-bookmarklet.ts --watch",
    "watch": "tsc --watch",
    "deploy": "git push dokku master",
    "ts-check": "tsc --noEmit",
    "lint": "eslint",
    "lint-fix": "npm run lint -- --fix",
    "lint-staged": "lint-staged",
    "postinstall": "patch-package",
    "prepare": "husky",
    "prod": "TZ='America/New_York' node ./dist/index.js",
    "start": "TZ='America/New_York' NODE_ENV=development DEBUG=proxy nf start --procfile ./Procfile_dev --port 3333",
    "dev": "TZ='America/New_York' DEBUG=proxy,proxy:verbose nodemon ./index.ts",
    "run-script": "NODE_ENV=development DEBUG=proxy node ./scripts/run-script.js",
    "run-script-test": "TEST=true NODE_ENV=development DEBUG=proxy node ./scripts/run-script.js",
    "test": "npm run ts-check && TZ='America/New_York' vitest run --coverage",
    "test-single": "NODE_ENV=development TZ='America/New_York' vitest run",
    "test-watch": "TZ='America/New_York' vitest"
  },
  "lint-staged": {
    "*.{js,ts}": "npm run lint"
  },
  "nodemonConfig": {
    "ignore": [
      "*.json"
    ]
  },
  "dependencies": {
    "axios": "^1.13.2",
    "body-parser": "^2.2.1",
    "bree": "^9.2.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.19",
    "debug": "^4.4.3",
    "deep-equal": "^2.2.3",
    "express": "^5.2.1",
    "express-handlebars": "^8.0.4",
    "firebase-admin": "^13.6.0",
    "fs-extra": "^11.3.2",
    "minimist": "^1.2.8",
    "mixpanel": "^0.19.1",
    "morgan": "^1.10.1",
    "rss-parser": "^3.13.0",
    "socket.io": "^4.8.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@notionhq/client": "^5.9.0",
    "@types/body-parser": "^1.19.6",
    "@types/debug": "^4.1.12",
    "@types/deep-equal": "^1.0.4",
    "@types/express": "^5.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/minimist": "^1.2.5",
    "@types/mock-fs": "^4.13.4",
    "@types/morgan": "^1.9.10",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitest/coverage-v8": "^4.0.15",
    "babel-eslint": "^10.1.0",
    "esbuild": "^0.27.0",
    "esbuild-plugin-bookmarklet": "^1.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-crb": "^2.0.7",
    "foreman": "^3.0.1",
    "get-port": "^7.1.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "mock-fs": "^5.5.0",
    "nock": "^14.0.10",
    "nodemon": "^3.1.11",
    "notion-to-md": "3.1.1",
    "patch-package": "^8.0.1",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.15"
  }
}
