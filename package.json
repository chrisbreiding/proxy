{
  "name": "proxy",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "16.18.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "nodemon ./index.ts",
    "prod": "node ./dist/index.js",
    "postinstall": "patch-package",
    "lint": "eslint *.ts lib/*.ts lib/**/*.ts lib/**/*.js --fix",
    "start": "NODE_ENV=development nf start --procfile ./Procfile_dev --port 3333",
    "get-block-id": "NODE_ENV=development node ./lib/notion/get-block-id.js",
    "add-year": "NODE_ENV=development node ./lib/notion/add-year.js",
    "year-in-review": "NODE_ENV=development node ./lib/notion/year-in-review.js",
    "dev-script": "NODE_ENV=development node ./scripts/run-script",
    "daytime-hourly": "node ./scripts/run-daytime-hourly",
    "daytime-quarter-hourly": "node ./scripts/run-daytime-quarter-hourly",
    "nightly": "node ./scripts/run-nightly"
  },
  "nodemonConfig": {
    "ignore": [
      "*.json"
    ]
  },
  "dependencies": {
    "axios": "^1.1.3",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.5",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "express-handlebars": "^6.0.6",
    "fs-extra": "^10.1.0",
    "minimist": "^1.2.7",
    "morgan": "^1.10.0",
    "patch-package": "^6.4.7",
    "socket.io": "^3.1.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@notionhq/client": "^2.2.1",
    "@types/body-parser": "^1.19.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.14",
    "@types/fs-extra": "^9.0.13",
    "@types/minimist": "^1.2.2",
    "@types/morgan": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-crb": "^2.0.3",
    "foreman": "^2.0.0",
    "nodemon": "^1.19.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
