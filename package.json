{
  "name": "proxy",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "16.18.0"
  },
  "scripts": {
    "dev": "nodemon ./index.js",
    "prod": "node ./index.js",
    "lint": "eslint *.js lib/*.js --fix",
    "test": "vitest",
    "start": "NODE_ENV=development nf start --procfile ./Procfile_dev --port 3333",
    "dev-script": "NODE_ENV=development node ./scripts/run-script",
    "daytime-hourly": "node ./scripts/run-daytime-hourly",
    "daytime-quarter-hourly": "node ./scripts/run-daytime-quarter-hourly",
    "nightly": "node ./scripts/run-nightly"
  },
  "nodemonConfig": {
    "ignore": [
      "*.json"
    ]
  },
  "dependencies": {
    "axios": "^1.1.3",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.6",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "express-handlebars": "^6.0.6",
    "fs-extra": "^10.1.0",
    "minimist": "^1.2.7",
    "morgan": "^1.10.0",
    "socket.io": "^4.5.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-crb": "^2.0.3",
    "foreman": "^3.0.1",
    "get-port": "^6.1.2",
    "nock": "^13.2.9",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vitest": "^0.24.3"
  }
